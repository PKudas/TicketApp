<%= form_with(model: ticket, local: true) do |form| %>


<div class="field">
    <%= form.hidden_field :event_id %>
</div>


<div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
</div>

<div class="field">
    <%= form.label :seat_id_seq %>
    <%= form.text_field :seat_id_seq %>
</div>

<div class="field">
    <%= form.label :address %>
    <%= form.text_area :address %>
</div>

<div class="field">
    <%= form.label :price %>
    <%= form.text_field :price %>
</div>

<div class="field">
    <%= form.label :email_address %>
    <%= form.text_field :email_address %>
</div>

<div class="field">
    <%= form.label :phone %>
    <%= form.text_field :phone %>
</div>

<div class="actions">
    <%= form.submit %>
</div>
<% end %>
