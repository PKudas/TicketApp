<%= bootstrap_form_with model: ticket,local:true  do |form| %>


  <div class="field-width">
        <%= form.label 'Wydarzenie' %>
        <%= form.collection_select(:event_id, Event.all, :id, :artist) %>
  </div>

  <div class="field-width">
    <%= form.text_field :name %>
  </div>

  <div class="field-width">
    <%= form.text_field :seat_id_seq %>
  </div>

  <div class="field-width">
    <%= form.text_area :address %>
  </div>

  <div class="field-width">
    <%= form.text_field :price %>
  </div>

  <div class="field-width">
    <%= form.text_field :email_address %>
  </div>

  <div class="field-width">
    <%= form.text_field :phone %>
  </div>

  <div class="actions">
    <%= form.primary %>
  </div>
<% end %>
